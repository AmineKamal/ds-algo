{
  "name": "@aminekamal/ds-algo",
  "version": "1.0.5",
  "description": "A collection of data structures and algorithms.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmineKamal/ds-algo.git"
  },
  "keywords": [
    "Algorithms",
    "Data Structures"
  ],
  "author": "Amine Kamal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AmineKamal/ds-algo/issues"
  },
  "homepage": "https://github.com/AmineKamal/ds-algo#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  }
}
